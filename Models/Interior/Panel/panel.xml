<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ASI -->
  <model>
    <path>Aircraft/Boeing-P26/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m> -0.011 </x-m>
      <y-m> -0.070 </y-m>
      <z-m>  0.052 </z-m>
    </offsets>
  </model>
  
  <!-- MAGNETIC COMPASS -->
  <model>
    <path>Aircraft/Boeing-P26/Models/Interior/Panel/Instruments/MagneticCompass/compas.xml</path>
    <offsets>
      <x-m> -0.01500 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.05060 </z-m>
    </offsets>
  </model>
  
  <!-- RPM -->
  <model>
    <path>Aircraft/Boeing-P26/Models/Interior/Panel/Instruments/rpm/rpm.xml</path>
    <offsets>
