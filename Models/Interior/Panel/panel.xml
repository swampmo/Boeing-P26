    <path>Aircraft/Boeing-P26/Models/Interior/Panel/Instruments/brakes/brakes.xml</path>
    <offsets>
      <x-m> -0.01389 </x-m>
      <y-m> -0.21644 </y-m>
      <z-m> -0.10494 </z-m>
    </offsets>
